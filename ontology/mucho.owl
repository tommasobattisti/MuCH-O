        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/TextFragment"/>
        <rdfs:comment xml:lang="en">Specifies that a Text has a TextFragment associated with it. This property represents the relationship between a Text, such as lyrics or written text, and its corresponding TextFragment. It indicates that the Text contains or includes the mentioned TextFragment, which represents a portion or segment of the overall Text.</rdfs:comment>
        <rdfs:label xml:lang="en">Has text part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/isGenreOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/isGenreOf">
