        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicArtist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/nickname -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/nickname">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/name"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/text -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/text">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">text</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/title -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/title">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/ns/prov#Derivation -->

    <owl:Class rdf:about="http://www.w3.org/ns/prov#Derivation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#EntityInfluence"/>
        <rdfs:comment xml:lang="en">An instance of prov:Derivation provides additional descriptions about the binary prov:wasDerivedFrom relation from some derived prov:Entity to another prov:Entity from which it was derived. For example, :chewed_bubble_gum prov:wasDerivedFrom :unwrapped_bubble_gum; prov:qualifiedDerivation [ a prov:Derivation; prov:entity :unwrapped_bubble_gum; :foo :bar ].</rdfs:comment>
        <rdfs:comment xml:lang="en">The more specific forms of prov:Derivation should be asserted if they apply.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>Derivation</rdfs:label>
        <prov:category>qualified</prov:category>
        <prov:component>derivations</prov:component>
        <prov:constraints rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/TR/2013/REC-prov-constraints-20130430/#prov-dm-constraints-fig</prov:constraints>
        <prov:definition xml:lang="en">A derivation is a transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity.</prov:definition>
        <prov:dm rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-Derivation</prov:dm>
        <prov:n rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/TR/2013/REC-prov-n-20130430/#Derivation-Relation</prov:n>
        <prov:unqualifiedForm rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/prov#EntityInfluence -->

    <owl:Class rdf:about="http://www.w3.org/ns/prov#EntityInfluence">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Influence"/>
        <rdfs:comment xml:lang="en">EntityInfluence provides additional descriptions of an Entity&apos;s binary influence upon any other kind of resource. Instances of EntityInfluence use the prov:entity property to cite the influencing Entity.</rdfs:comment>
        <rdfs:comment xml:lang="en">It is not recommended that the type EntityInfluence be asserted without also asserting one of its more specific subclasses.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>EntityInfluence</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/prov#entity"/>
        <prov:category>qualified</prov:category>
        <prov:editorsDefinition xml:lang="en">EntityInfluence is the capacity of an entity to have an effect on the character, development, or behavior of another by means of usage, start, end, derivation, or other. </prov:editorsDefinition>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/prov#Influence -->

    <owl:Class rdf:about="http://www.w3.org/ns/prov#Influence">
        <rdfs:comment xml:lang="en">An instance of prov:Influence provides additional descriptions about the binary prov:wasInfluencedBy relation from some influenced Activity, Entity, or Agent to the influencing Activity, Entity, or Agent. For example, :stomach_ache prov:wasInfluencedBy :spoon; prov:qualifiedInfluence [ a prov:Influence; prov:entity :spoon; :foo :bar ] . Because prov:Influence is a broad relation, the more specific relations (Communication, Delegation, End, etc.) should be used when applicable.</rdfs:comment>
        <rdfs:comment xml:lang="en">Because prov:Influence is a broad relation, its most specific subclasses (e.g. prov:Communication, prov:Delegation, prov:End, prov:Revision, etc.) should be used when applicable.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov-o#"/>
        <rdfs:label>Influence</rdfs:label>
        <prov:category>qualified</prov:category>
        <prov:component>derivations</prov:component>
        <prov:definition xml:lang="en">Influence is the capacity of an entity, activity, or agent to have an effect on the character, development, or behavior of another by means of usage, start, end, generation, invalidation, communication, derivation, attribution, association, or delegation.</prov:definition>
        <prov:dm rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-influence</prov:dm>
        <prov:n rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/TR/2013/REC-prov-n-20130430/#expression-influence</prov:n>
        <prov:unqualifiedForm rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Adaptation -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Adaptation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Derivation"/>
        <rdfs:comment xml:lang="en">An instance of Adaptation provides additional descriptions about the binary isAdaptationOf relation from some derived entity to another entity from which it was adapted (typically involving an entity with another medium).
For example, it can be used for specifying the relation expressing the fact that the film &quot;The Shining&quot; was adapted from the homonymous book.</rdfs:comment>
        <rdfs:label xml:lang="en">Adaptation</rdfs:label>
        <prov:category xml:lang="en">qualified</prov:category>
        <prov:definition xml:lang="en">An adaptation is the act of adjusting some existent entity in order to make it fit to different conditions, in this case determined by the medium on which it is delivered. The adaptation process ends with the creation of a new entity that has different structural characteristics but still represents the same nucleus of meaning.</prov:definition>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Allusion -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Allusion">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Reference"/>
        <rdfs:comment xml:lang="en">An instance of Allusion provides additional descriptions about the binary alludesTo relation from some alluding entity to another entity it has been alluded to.</rdfs:comment>
        <rdfs:label xml:lang="en">Allusion</rdfs:label>
        <prov:category xml:lang="en">qualified</prov:category>
        <prov:definition xml:lang="en">An Allusion is an indirect reference to another entity intended as a resemblance or an indirect call to attention aiming at suggesting a relationship between the alluding entity and the one it has been alluded to.</prov:definition>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Annotation -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Annotation">
        <rdfs:comment xml:lang="en">It specifies an annotation related to a Text or a TextFragment entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#AudiovisualEntity -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#AudiovisualEntity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationObject"/>
        <rdfs:comment xml:lang="en">This class represents an Information Object whose production, existence, and fruition needs the processing of both a soud and a visual component (e.g., films, television products, videoclips, etc.).</rdfs:comment>
        <rdfs:label xml:lang="en">Audiovisual Entity</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Basis -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Basis">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Derivation"/>
        <rdfs:comment xml:lang="en">An instance of Basis provides additional descriptions about the binary isBasedOn relation from some derived entity to another entity from which it was based on. It does not necessarily involve the change of medium bewteeen the two entities.</rdfs:comment>
        <rdfs:label xml:lang="en">Basis</rdfs:label>
        <prov:category xml:lang="en">qualified</prov:category>
        <prov:definition xml:lang="en">Being based on something means to have an existent entity acting as the primary source for shaping a new entity maintaining some arbitrary connections between the different levels of significance.</prov:definition>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Citation -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Citation">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Reference"/>
        <rdfs:comment xml:lang="en">An instance of Citation provides additional descriptions about the binary cites relation from some citing entity to another cited entity. It should be intended as a synonym of quotation.</rdfs:comment>
        <rdfs:label xml:lang="en">Citation</rdfs:label>
        <prov:category xml:lang="en">qualified</prov:category>
        <prov:definition xml:lang="en">A Citation is here intended with its meanong of quotation from an entity to another. It should be intended as an explict reuse of some part of another entity&apos;s content rather than as a general reference to it.</prov:definition>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Group -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Group">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasMember"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/polifonia/ontology/core/Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/name"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A named group or collective formed by two or more people that act collaboratively towards the achievement of a particular goal (e.g., an art collective aiming at producing art pieces, a band aiming at publishing albums and performing live, etc.).
Differs from two or more people collaborating without forming a recognised and named group that can identify them as a collective.</rdfs:comment>
        <rdfs:label xml:lang="en">Group</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Inspiration -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Inspiration">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#EntityInfluence"/>
        <rdfs:comment xml:lang="en">An instance of inspiration provides additional descriptions about the binary wasInspiredBy relation between two entities.</rdfs:comment>
        <rdfs:label xml:lang="en">Inspiration</rdfs:label>
        <prov:category xml:lang="en">qualified</prov:category>
        <prov:definition xml:lang="en">An inspiration is the process of being stimulated by some existent entity to do, produce, or create some new entity. It consists getting from something  a new or creative idea that can be applied in the creation of something new.</prov:definition>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#LiteraryEntity -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#LiteraryEntity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationObject"/>
        <rdfs:comment xml:lang="en">This class represents an Information Object intended as a written work related to the concept of literature intended as both any collection of written works and as an art form (e.g., novels, poems, letters, diaries, essays, plays, etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">Literary Entity</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Mention -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Mention">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Reference"/>
        <rdfs:comment xml:lang="en">An instance of Mention provides additional descriptions about the binary mentions relation from some mentioning entity to another mentioned entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Mention</rdfs:label>
        <prov:category xml:lang="en">qualified</prov:category>
        <prov:definition xml:lang="en">A mention is an explicit reference to something (another entity) but without going into detail or reporting part of the mentioned entity&apos;s content.</prov:definition>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#MusicAlbum -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#MusicAlbum">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:comment xml:lang="en">A Music Album is a music entity abstracting the concept of a collection of tracks (songs).</rdfs:comment>
        <rdfs:label xml:lang="en">Music Album</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Reference -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Reference">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#EntityInfluence"/>
        <rdfs:comment xml:lang="en">An instance of Reference provides additional descriptions about the binary references relation from some referencing entity to another referenced entity.</rdfs:comment>
        <rdfs:comment xml:lang="en">The more specific forms of Reference should be asserted if they apply.</rdfs:comment>
        <rdfs:label xml:lang="en">Reference</rdfs:label>
        <prov:category xml:lang="en">qualified</prov:category>
        <prov:definition xml:lang="en">A reference is any kind of direct or indirect mention, allusion, or citation to some external entity with respect to the referencing one.</prov:definition>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Song -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Song">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isPartOfMusicEntity"/>
                <owl:someValuesFrom rdf:resource="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#MusicAlbum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Song abstracts the concept of audio recording representing a music entity intended as a single track which may be part of an album or other kind of collections.</rdfs:comment>
        <rdfs:label xml:lang="en">Song</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#VisualArtEntity -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#VisualArtEntity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationObject"/>
        <rdfs:comment xml:lang="en">This class represents an Information Object whose production, existence, and fruition is primarily based on visual characteristics (e.g., paintings, photographs, sculptures, etc.).
Differs from the common definition of visual arts since it does not include audiovisual works.</rdfs:comment>
        <rdfs:label xml:lang="en">Viausl Art Entity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Agent -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/core/Agent">
        <rdfs:comment xml:lang="en">Any agentive Object, either physical (e.g. a person, a robot), or social (e.g. a corporation, an institution, a community)</rdfs:comment>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/InformationObject -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/core/InformationObject">
        <rdfs:comment xml:lang="en">A piece of information, such as a musical composition, a text, a word, a picture, independently from how it is concretely and phisically realized.</rdfs:comment>
        <rdfs:label xml:lang="en">Information object</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Person -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/core/Person">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:label xml:lang="en">Person</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/Lyrics -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/Lyrics">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Text"/>
        <rdfs:comment xml:lang="en">The textual component of a musical composition that consists of the words, verses, or poetic expressions sung or spoken during the performance. Lyrics provide the vocal content and convey the narrative, emotions, or messages expressed through the music. They encompass the written representation of the lyrical content, which may be organized into lines, stanzas, or verses, forming an integral part of the composition.</rdfs:comment>
        <rdfs:label xml:lang="en">Lyrics</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/MusicArtist -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/MusicArtist">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasMember"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Musician"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasGenre"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicGenre"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/name"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/name"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An individual or group involved in creating, performing, or producing music. It encompasses musicians, singers, bands, composers, conductors, producers, and other artistic entities within the music industry.</rdfs:comment>
        <rdfs:label xml:lang="en">Music Artist</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/MusicEnsenble -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/MusicEnsenble">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/tommasobattisti/MuCH-O/main/ontology/mucho.owl#Group"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicArtist"/>
        <rdfs:comment xml:lang="en">A group or collective of musicians who perform together as an ensemble. It encompasses various types of musical groups, such as orchestras, bands, choirs, chamber ensembles, and jazz ensembles.</rdfs:comment>
        <rdfs:label xml:lang="en">Music Ensenble</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/MusicEntity -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/MusicEntity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasPart"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasMusicEntityPart"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This class represents an Information Object, which is defined as the sum of all the elements that make up a piece of music. A music entity abstract a musical work, as it may results from a composition process, an improvisation, or be the result of non-written transmission.</rdfs:comment>
        <rdfs:label xml:lang="en">Music Entity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/MusicGenre -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/MusicGenre">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isGenreOf"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicArtist"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A specific category or classification of music that shares common characteristics, styles, or cultural origins. It encompasses distinct musical traditions, styles, or movements that define a particular genre.</rdfs:comment>
        <rdfs:label xml:lang="en">Music Genre</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/Musician -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/Musician">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Person"/>
