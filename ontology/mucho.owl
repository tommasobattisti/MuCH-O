    <!-- https://w3id.org/polifonia/ontology/music-meta/hasGenre -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/hasGenre">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isGenreOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicArtist"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicGenre"/>
        <rdfs:comment xml:lang="en">Specifies the genre or genres associated with a MusicArtist. This property represents the relationship between a MusicArtist and the MusicGenre(s) that are attributed or associated with their artistic output or musical style. It indicates the genre(s) that best describe the musical genre(s) or genres in which the MusicArtist operates or specializes.</rdfs:comment>
        <rdfs:label xml:lang="en">Has genre</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/hasMusicEntityPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/hasMusicEntityPart">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isPartOfMusicEntity"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Text"/>
        <rdfs:comment xml:lang="en">Specifies the relationship between a MusicEntity and its constituent parts or components. This property represents the connection between a MusicEntity and the individual parts or components that comprise it. It indicates that the MusicEntity is composed of or includes the mentioned parts, which may consist of specific sections, movements, instrumental or vocal parts, or other structural elements that contribute to the overall composition.</rdfs:comment>
        <rdfs:label xml:lang="en">Has music entity part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/hasSource -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/hasSource">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ReflexiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Text"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Text"/>
        <rdfs:label xml:lang="en">Has source</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/hasTextFragment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/hasTextFragment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isTextFragmentOf"/>
